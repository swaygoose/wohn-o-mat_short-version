{
	"theses": {
		"0": {
			"l": "Ich lege bei meiner Wohnung einen hohen Wert auf ein ruhiges Umfeld.",
			"s": "Ungestörtheit",
			"x": ""
		},
		"1": {
			"l": "Ich finde, eines der wichtigsten Dinge beim Wohnen ist mein eigener Rückzugsbereich.",
			"s": "Privatsphäre",
			"x": ""
		},
		"2": {
			"l": "Ich selbst brauche wenig Platz im Alltag. Habe ich jedoch Besuch, bin ich auf weitere Räume angewiesen.",
			"s": "Gemeinschaftsräume",
			"x": ""
		},
		"3": {
			"l": "Ich wünsche mir ein Zuhause, das auch Raum für gemeinschaftliche Aktivitäten bietet.",
			"s": "Aktivitäten",
			"x": ""
		},
		"4": {
			"l": "Ich habe Interesse daran, gemeinschaftliche Projekte (z. B. Garten, Werkstatt, Veranstaltungen) mitzugestalten.",
			"s": "Gemeinschaftliche Projekte",
			"x": ""
		},
		"5": {
			"l": "Ich möchte nicht mehrfach im Leben umziehen, sondern eine möglichst langfristige Lösung finden.",
			"s": "Langfristige Lösung",
			"x": ""
		},
		"6": {
			"l": "Ich will nicht nur für mich alleine wohnen, sondern Teil einer engen, sozialen Gemeinschaft sein. ",
			"s": "Soziales Umfeld",
			"x": ""
		},
		"7": {
			"l": "Mir ist es sehr wichtig, die Menschen, die um mich herum wohnen, zu kennen.",
			"s": "Nachbarn",
			"x": ""
		},
		"8": {
			"l": "Mir ist wichtig, einen eigenen separaten Raum zu haben, in welchem ich meine Hobbies ausüben kann.",
			"s": "Hobbies",
			"x": ""
		},
		"9": {
			"l": "Ich bin bereit bei einem etwaigen Umzug viele meines Besitzes nicht mitzunehmen.",
			"s": "Umzug",
			"x": ""
		},
		"10": {
			"l": "Ich kann mir vorstellen, wichtige Lebensbereiche in meiner Wohnung mit anderen Menschen zu teilen.",
			"s": "Teilen",
			"x": ""
		},
		"11": {
			"l": "Ich habe kein Problem damit, mit anderen Menschen, die nicht meine eigene Familie sind, zusammenzuwohnen.",
			"s": "Mitbewohner:innen",
			"x": ""
		},
		"12": {
			"l": "Ich bin der Meinung, dass das Leben in Gemeinschaft eine große Bereicherung für mich ist.",
			"s": "Gemeinschaftswohnen",
			"x": ""
		},
		"13": {
			"l": "Ich möchte mit Menschen verschiedenen Alters in einer Gemeinschaft wohnen.",
			"s": "Altersstruktur",
			"x": ""
		},
		"14": {
			"l": "Ich habe kein Problem damit, mir z.B. Waschmaschine oder Auto mit anderen zu teilen.",
			"s": "Teilen II",
			"x": ""
		},
		"15": {
			"l": "Ich bin auf alltägliche Hilfe im Haushalt angewiesen.",
			"s": "Alltagshilfe",
			"x": ""
		},
		"16": {
			"l": "Mir ist es wichtig, dass Pflegepersonal im Haus ansässig ist.",
			"s": "Pflege",
			"x": ""
		}
	},
